#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/akrij/SolidDark/soliddark/rashomon/node_modules/.pnpm/@electron+rebuild@4.0.3/node_modules/@electron/rebuild/lib/node_modules:/home/akrij/SolidDark/soliddark/rashomon/node_modules/.pnpm/@electron+rebuild@4.0.3/node_modules/@electron/rebuild/node_modules:/home/akrij/SolidDark/soliddark/rashomon/node_modules/.pnpm/@electron+rebuild@4.0.3/node_modules/@electron/node_modules:/home/akrij/SolidDark/soliddark/rashomon/node_modules/.pnpm/@electron+rebuild@4.0.3/node_modules:/home/akrij/SolidDark/soliddark/rashomon/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/akrij/SolidDark/soliddark/rashomon/node_modules/.pnpm/@electron+rebuild@4.0.3/node_modules/@electron/rebuild/lib/node_modules:/home/akrij/SolidDark/soliddark/rashomon/node_modules/.pnpm/@electron+rebuild@4.0.3/node_modules/@electron/rebuild/node_modules:/home/akrij/SolidDark/soliddark/rashomon/node_modules/.pnpm/@electron+rebuild@4.0.3/node_modules/@electron/node_modules:/home/akrij/SolidDark/soliddark/rashomon/node_modules/.pnpm/@electron+rebuild@4.0.3/node_modules:/home/akrij/SolidDark/soliddark/rashomon/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@electron+rebuild@4.0.3/node_modules/@electron/rebuild/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../@electron+rebuild@4.0.3/node_modules/@electron/rebuild/lib/cli.js" "$@"
fi
